<keymap version="1" name="my custom" parent="$default">
  <action id="Back">
    <mouse-shortcut keystroke="button4" />
  </action>
  <action id="CloseActiveTab" />
  <action id="CloseContent">
    <keyboard-shortcut first-keystroke="ctrl f4" />
    <keyboard-shortcut first-keystroke="shift w" />
  </action>
  <action id="EditorCloneCaretAbove">
    <keyboard-shortcut first-keystroke="alt w" />
  </action>
  <action id="EditorCloneCaretBelow">
    <keyboard-shortcut first-keystroke="alt s" />
  </action>
  <action id="EditorDecreaseFontSize">
    <mouse-shortcut keystroke="control button142" />
  </action>
  <action id="EditorIncreaseFontSize">
    <mouse-shortcut keystroke="control button143" />
  </action>
  <action id="EditorNextWordInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl alt right" />
  </action>
  <action id="EditorPreviousWordInDifferentHumpsMode">
    <keyboard-shortcut first-keystroke="ctrl alt left" />
  </action>
  <action id="Forward">
    <mouse-shortcut keystroke="button5" />
  </action>
</keymap>